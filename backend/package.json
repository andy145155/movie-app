{
  "dependencies": {
    "serverless": "^3.25.0",
    "serverless-fargate": "^0.0.11"
  },
  "devDependencies": {
    "serverless-plugin-warmup": "^8.2.1",
    "serverless-stack-output": "^0.2.3"
  },
  "scripts": {
    "deployLambdaApi": "sls deploy -c serverless.api.yml",
    "deployLambdaDataProcessing": "sls deploy -c serverless.dataProcessing.yml",
    "deployTriggeredTasks": "cd /triggered-tasks && sls deploy",
